{
  "name": "My miscellaneous content scripts",
  "version": "0.1",
  "permissions": [
    "http://127.0.0.1:8180/*",
    "http://127.0.0.1:8080/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://127.0.0.1:8180/*",
        "http://127.0.0.1:8080/*"
      ],
      "js": ["jquery-2.1.0.js", "misc-content-scripts.js"],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2
}
